{
  "family": "react-ecs-task-definition",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512",
  "containerDefinitions": [
    {
      "name": "react-ecs-nginx",
      "image": "MY_ECR_REGISTRY/MY_ECR_REPOSITORY:latest",
      "portMappings": [
        {
          "containerPort": 80,
          "hostPort": 80,
          "protocol": "tcp"
        }
      ],
      "essential": true,
      "environment": [
        {
          "name": "VITE_SAMPLE_KEY",
          "value": ""
        }
      ]
    }
  ]
}
